<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amil.my_shopping" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".shopping_menu"
            android:label="@string/title_activity_shopping_menu" >
        </activity>
        <activity
            android:name=".menu_list"
            android:label="@string/title_activity_menu_list" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name=".shopping_places"
            android:label="@string/title_activity_shopping_places" >
        </activity>
        <activity
            android:name=".shopping_brand"
            android:label="@string/title_activity_shopping_brand" >
        </activity>
        <activity
            android:name=".shpping_stations"
            android:label="@string/title_activity_shpping_stations" >
        </activity>
        <activity
            android:name=".shoppingplaces"
            android:label="@string/title_activity_shoppingplaces" >
        </activity>
        <activity
            android:name=".seach_brandplace"
            android:label="@string/title_activity_seach_brandplace" >
        </activity>
    </application>

</manifest>
